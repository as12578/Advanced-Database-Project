// multiple cycle scenario
// Deadlock detected in transactions ['T1', 'T2']
//T2  will be aborted
//Deadlock detected in transactions ['T4', 'T5']
//T5 Transaction will be aborted
begin(T1)
begin(T2)
begin(T3)
begin(T4)
begin(T5)
R(T3,x3)
R(T4,x4)
R(T5,x5)
R(T1,x1)
R(T2,x2)
R(T2,x6)
R(T2,x7)
R(T2,x10)
W(T1,x2,10)
W(T2,x1,20)
W(T3,x3,30)
W(T4,x5,40)
W(T5,x4,50)
R(T2,x8)
R(T2,x9)
end(T5)
end(T4)
end(T3)
end(T2)
end(T1)