begin(T1)
beginRO(T2)
begin(T3)
begin(T4)
R(T1,x1)
W(T1,x1,101)
# T3 will wait for T1 on x1
R(T3, x1) 
R(T1,x1)
R(T4, x1)
W(T1,x2,102) 
R(T1,x2)
# T2 Will use MVRC for x1. Should read 10
R(T2,x1)
dump(x2)
# T3, T4 wil read x1: Should read 101
end(T1)
# T2 Will use MVRC for x1. Should read 10
R(T2,x1)
# T3 wil read x2: Should read 102
R(T3, x2)
# T3 will wait since 2 transactions hold shared lock
W(T3, x1, 150)
# T3 will write
end(T4)
# T3 will read 150
R(T3, x1)
dump()
dump(x1)
dump(x2)
dump(5)
# fail(6)
# recover(6)
